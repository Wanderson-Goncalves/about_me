# Configurações padrão para todos os arquivos
[*]
indent_style = space
indent_size = 4
tab_width = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

# Configurações específicas para arquivos JavaScript
[*.js]
indent_size = 4
"emmet.includeLanguages": {
    "javascript": "javascriptreact"
}
